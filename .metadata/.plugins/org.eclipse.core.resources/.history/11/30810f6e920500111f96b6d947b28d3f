package castingTask;

public class CastingTask02 {
   public static void main(String[] args) {
//      "8.43" + 2.59를 더해서 10이 나오도록 출력하기
	   double num1 = 0.0;
	   double num2 = 0.0;
	   int result = 0;
	   String resultFormat = ""
	   
	   num1 = Double.parseDouble("8.43");
	   num2 = 2.59;
	   
	   result = (int)num1+(int)num2;
	   resultFormat = 
	   System.out.println(result);
   }
}
