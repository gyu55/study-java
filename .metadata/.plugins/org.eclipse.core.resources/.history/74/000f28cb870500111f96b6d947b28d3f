package castingTest;

public class CastingTest {
	public static void main(String[] args) {
//		형변환
		
//		자동 형변환
//		정수 + 정수 = 정수
		System.out.println(1 + 2);
		System.out.println(10 / 3); // 정수끼리 연산하는 것이기 때문에 값은 3
		
//		정수 + 실수 = 실수(double)
		System.out.println(2 + 0.5);
		
		System.out.println((int)8.9); // 실수를 정수로 강제 형변환시, 소수점자리 버림
	}
}
