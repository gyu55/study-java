package formatTask;

public class FormatTask01 {
	public static void main(String[] args) {
//		식을 완성시키시오.
//		10 + 20 = 30
//		7 * 8 = 56
//		6 * 4 = 24
		
		System.out.printf("%d + %d = %d\n", 10, 20, 10 + 20);
		System.out.printf("%d * %d = %d\n", 7, 8, 7 * 8);
		System.out.printf("%d * %d = %d", 6, 4, 6 * 4);
	}
}
